{
  "name": "uma/schema-bundle",
  "description": "Provides annotations for easily placing JsonSchemas on top of routes",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["symfony", "json-schema"],
  "homepage": "https://github.com/1ma/UMASchemaBundle",
  "support": {
    "issues": "https://github.com/1ma/UMASchemaBundle/issues",
    "source": "https://github.com/1ma/UMASchemaBundle"
  },
  "abandoned": true,
  "require": {
    "php": ">=5.5.9",

    "doctrine/annotations": "^1.0",
    "justinrainbow/json-schema": "^3.0",
    "symfony/config": "^2.7|^3.0",
    "symfony/dependency-injection": "^2.7|^3.0",
    "symfony/http-kernel": "^2.7|^3.0"
  },
  "require-dev": {
    "symfony/browser-kit": "^2.7|^3.0",
    "symfony/framework-bundle": "^2.7|^3.0"
  },
  "autoload": {
    "psr-4": {
      "UMA\\SchemaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TestProject\\": "tests/TestProject/src/",
      "UMA\\Tests\\SchemaBundle\\": "tests/Functional"
    }
  }
}
